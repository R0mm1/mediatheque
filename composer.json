{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.1.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "api-platform/api-pack": "^1.3",
        "doctrine/annotations": "^1.6",
        "sensio/framework-extra-bundle": "^6.0",
        "symfony/asset": "^6.0",
        "symfony/console": "^6.0",
        "symfony/flex": "^6.0",
        "symfony/framework-bundle": "^6.0",
        "symfony/http-kernel": "^6.0",
        "symfony/orm-pack": "^1.0",
        "symfony/security-bundle": "^6.0",
        "symfony/swiftmailer-bundle": "^6.0",
        "symfony/validator": "^6.0",
        "symfony/yaml": "^6.0",
        "vich/uploader-bundle": "^1.19"
    },
    "require-dev": {
        "symfony/debug-bundle": "^6.0",
        "symfony/dotenv": "^6.0",
        "symfony/maker-bundle": "^6.0",
        "symfony/profiler-pack": "^6.0",
        "symfony/var-dumper": "^6.0",
        "symfony/web-server-bundle": "^6.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^6.0"
        }
    }
}
